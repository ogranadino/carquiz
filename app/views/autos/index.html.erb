<div class="page-header">
  <h1>Userdata</h1>
</div>

<p>Your Information</p>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Brand</th>
      <th>Model</th>
      <th>Year</th>
      <th>Price low</th>
      <th>Price high</th>
      <th>Mpg city</th>
      <th>Mpg hwy</th>
      <th>Comment</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @autos.each do |auto| %>
      <tr>
        <td><%= auto.brand %></td>
        <td><%= auto.model %></td>
        <td><%= auto.year %></td>
        <td><%= auto.price_low %></td>
        <td><%= auto.price_high %></td>
        <td><%= auto.mpg_city %></td>
        <td><%= auto.mpg_hwy %></td>
        <td><%= auto.comment %></td>
        <td>
          <a href="/autos/<%= auto.id %>" class="btn btn-primary">
            <i class="fa fa-search-plus"></i>
          </a>
        </td>
        <td>
          <a href="/autos/<%= auto.id %>/edit" class="btn btn-warning">
            <i class="fa fa-edit"></i>
          </a>
        </td>
        <td>
          <a href="/delete_auto/<%= auto.id %>" class="btn btn-danger">
            <i class="fa fa-trash-o"></i>
          </a>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

