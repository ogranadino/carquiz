<div class="page-header">
  <h1>Autousers</h1>
</div>

<p><a href="/autousers/new" class="btn btn-primary">Add a New Autouser</a></p>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Id auto</th>
      <th>Id user</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @autousers.each do |autouser| %>
      <tr>
        <td><%= autouser.id_auto %></td>
        <td><%= autouser.id_user %></td>
        <td>
          <a href="/autousers/<%= autouser.id %>" class="btn btn-primary">
            <i class="fa fa-search-plus"></i>
          </a>
        </td>
        <td>
          <a href="/autousers/<%= autouser.id %>/edit" class="btn btn-warning">
            <i class="fa fa-edit"></i>
          </a>
        </td>
        <td>
          <a href="/delete_autouser/<%= autouser.id %>" class="btn btn-danger">
            <i class="fa fa-trash-o"></i>
          </a>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

